{
  "compilerOptions": {    
    "target": "es5",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ] ,
    "jsx": "preserve",
    "module": "commonjs",
    
    "moduleResolution": "node",
    "resolveJsonModule": true ,
    "allowJs": true ,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true, 
    "noEmit": true ,
    "isolatedModules": true,
    "esModuleInterop": true ,
    "forceConsistentCasingInFileNames": true ,
    "strict": true ,
    "skipLibCheck": true
  },
  "exclude": ["node_modules"]
}
