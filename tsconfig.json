{
  "compilerOptions": {
		"target": "esnext",
		"lib": ["DOM", "DOM.Iterable", "ESNext"],
		"jsx": "preserve",
		"module": "commonjs",
		"checkJs": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"allowJs": true,
		"declaration": true,
		"declarationMap": false,
		"sourceMap": false,
		"isolatedModules": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
	},
  "exclude": ["node_modules"]
}
